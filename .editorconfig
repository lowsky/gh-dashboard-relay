# http://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{html}]
charset = utf-8

[*.js]
charset = utf-8

[package.json]
indent_size = 2
