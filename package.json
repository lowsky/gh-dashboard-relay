{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "github"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "echo you may want to run `yarn dev` for local development... ;  next start",
    "build:relay": "relay-compiler --src . --schema schema/schema.graphql --language typescript --watchman false --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' '**/schema/**' --include '**/container/**' '**/components/**' '**/pages/**' '**/relay/**' '**/queries/**'",
    "codegen": "node schema/wrapGraphQLSchema ; graphql-codegen --config schema/codegen.yml",
    "vercel-build": " yarn run vercel-build:without-chromatic ; yarn run chromatic-test",
    "vercel-build:without-chromatic": "yarn run build:relay ; yarn run build:storybook; yarn run build",
    "_lint": "eslint .",
    "test": "jest",
    "coverage": "npm run test -- --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:ci": "mkdir -p test-reports ; yarn run test:circleci",
    "test:vis": "start-server-and-test 'yarn run storybook -- --quiet --ci' 9009 'cypress run --record'",
    "test:storybook": "cypress run --record",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml npm test -- --ci --testResultsProcessor=./node_modules/jest-junit-reporter --all --watch=false --watchAll=false --forceExit",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "start-storybook -p 9009",
    "build:storybook": "build-storybook -o public/story-book --quiet"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "@octokit/graphql-schema": "10.58.0",
    "@octokit/rest": "18.8.0",
    "apollo-server-micro": "3.1.2",
    "bufferutil": "4.0.3",
    "bulma": "0.9.3",
    "graphql": "15.5.1",
    "is-plain-object": "5.0.0",
    "micro": "9.3.4",
    "next": "11.0.1",
    "next-images": "1.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router": "5.2.0",
    "react-router-config": "5.1.1",
    "relay-runtime": "11.0.2",
    "utf-8-validate": "5.0.5"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.16",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-resolvers": "1.20.0",
    "@graphql-tools/schema": "8.0.1",
    "@octokit/types": "6.19.0",
    "@storybook/addon-actions": "6.3.6",
    "@storybook/addon-essentials": "6.3.6",
    "@storybook/addon-links": "6.3.6",
    "@storybook/addon-storyshots": "6.3.6",
    "@storybook/react": "6.3.6",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.9",
    "@types/react-relay": "11.0.2",
    "@types/relay-runtime": "11.0.2",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-relay": "11.0.2",
    "chromatic": "5.9.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql": "15.5.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "react-relay": "11.0.2",
    "relay-compiler": "11.0.2",
    "relay-compiler-language-typescript": "14.0.0",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5",
    "webpack": "5.48.0"
  }
}
