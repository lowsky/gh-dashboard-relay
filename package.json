{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "dev": "webpack-dev-server --progress --colors --inline",
    "test": "NODE_ENV=test istanbul cover -v -i '**/*.js'  _mocha test/** -- --compilers js:babel-core/register",
    "test:circle": "XUNIT_FILE=$CIRCLE_TEST_REPORTS/mocha-xunit.xml npm run test -- -R xunit-file",
    "prestart": "webpack",
    "start": "node ."
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": "https://github.com/lowsky/dashboard",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "express": "^4.14.0",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-helper-builder-react-jsx": "^6.8.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-relay-plugin": "^0.10.0",
    "chai": "^3.5.0",
    "enzyme": "2.7.1",
    "eslint": "^3.13.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-graphql": "^0.6.0",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^6.9.0",
    "express-graphql": "^0.6.2",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "graphql-schema-from-introspection": "0.1.2",
    "istanbul": "^0.4.4",
    "jsdom": "^9.9.0",
    "jsx-loader": "^0.13.2",
    "minimatch": "^3.0.3",
    "mocha": "^3.2.0",
    "node-libs-browser": "^2.0.0",
    "nodemon": "^1.10.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-relay": "^0.10.0",
    "superagent": "^3.4.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.1",
    "xunit-file": "^1.0.0"
  }
