{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=8"
  },
  "proxy": "http://localhost:3000",
  "scripts": {
    "cra-start": "PORT=8080 react-app-rewired start",
    "cra-build": "react-app-rewired build",
    "relay": "relay-compiler --src ./src --schema ./src/relay/data/schema.graphql",
    "build": "npm run relay ; npm run cra-build",
    "lint": "eslint .",
    "dev": "yarn run cra-start",
    "test": "react-app-rewired test --env=jsdom --no-cache ",
    "coverage": "yarn run test --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml yarn test --ci --testResultsProcessor=./node_modules/jest-junit-reporter --forceExit",
    "start": "babel-node .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "cors": "2.8.4",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "helmet": "3.12.0",
    "react-scripts": "1.1.2",
    "relay-runtime": "1.5.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.15",
    "@storybook/addon-links": "3.3.15",
    "@storybook/addons": "3.3.15",
    "@storybook/react": "3.3.15",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-plugin-relay": "1.5.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "5.0.0",
    "eslint-plugin-graphql": "2.0.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "graphql": "0.13.2",
    "graphql-relay": "0.5.5",
    "jest-junit-reporter": "1.1.0",
    "prettier": "1.11.1",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-app-rewired": "1.5.0",
    "react-dom": "16.3.2",
    "react-relay": "1.5.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.3.2",
    "relay-compiler": "1.5.0",
    "xunit-file": "1.0.0"
  }
}
