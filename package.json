{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "type": "module",
  "keywords": [
    "graphql",
    "relay",
    "react",
    "suspense",
    "react suspense",
    "dashboard",
    "github"
  ],
  "engines": {},
  "scripts": {
    "dev": "next dev",
    "circle:setup": "vercel link -y --repo --debug --project gh-dashboard-relay -t $VERCEL_TOKEN ; vercel -t $VERCEL_TOKEN pull",
    "circle:deploy": "vercel -y -t $VERCEL_TOKEN deploy --prebuilt",
    "build": "yarn run codegen && yarn next build",
    "start": "echo you may want to run `yarn dev` for local development... &&  next start",
    "build:relay": "relay-compiler && prettier -w relay/__generated__ app/relay/[userName]/__generated__ app/relay/[userName]/[repoName]",
    "build:relay:watch": "relay-compiler -w",
    "build:apollo": "graphql-codegen",
    "build:apollo:watch": "graphql-codegen -w",
    "codegen": "yarn run build:relay && yarn run build:apollo",
    "format": "prettier . --cache -l -w",
    "vercel-build:without-chromatic": "yarn run build",
    "lint": "eslint .",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "storybook dev -p 9009",
    "build:storybook": "storybook build -o public/story-book --quiet"
  },
  "relay": {
    "src": ".",
    "eagerEsModules": true,
    "language": "typescript",
    "exclude": [
      "/.next/**",
      "**/stuff/**",
      "**/node_modules/**",
      "**/test/**",
      "**/apollo/**",
      "**/__generated__/**"
    ],
    "schema": "./schema.graphql"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@apollo/client": "^4.0.0",
    "@chakra-ui/react": "3.31.0",
    "@emotion/react": "11.14.0",
    "@next/mdx": "16.1.6",
    "@octokit/graphql-schema": "15.26.1",
    "graphql": "16.12.0",
    "meros": "1.3.2",
    "next": "16.1.4",
    "next-themes": "^0.4.3",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "1.15.2",
    "@chromatic-com/storybook": "5.0.0",
    "@eslint/compat": "2.0.2",
    "@eslint/js": "9.39.2",
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.1",
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/client-preset": "5.2.2",
    "@graphql-codegen/client-preset-swc-plugin": "0.2.0",
    "@graphql-codegen/fragment-matcher": "6.0.0",
    "@graphql-codegen/typescript-operations": "5.0.7",
    "@graphql-codegen/typescript-react-apollo": "4.4.0",
    "@graphql-eslint/eslint-plugin": "4.4.0",
    "@graphql-tools/schema": "10.0.31",
    "@next/eslint-plugin-next": "^16.0.0",
    "@parcel/watcher": "2.5.6",
    "@storybook/addon-a11y": "10.2.0",
    "@storybook/addon-docs": "10.2.0",
    "@storybook/addon-themes": "10.2.0",
    "@storybook/addon-vitest": "10.2.0",
    "@storybook/nextjs-vite": "10.2.0",
    "@types/react": "19.2.9",
    "@types/react-relay": "^18.2.0",
    "@types/relay-test-utils": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "chromatic": "13.3.5",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-relay": "2.0.0",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-storybook": "10.2.0",
    "globals": "17.1.0",
    "nextjs-toploader": "3.9.17",
    "playwright": "^1.57.0",
    "prettier": "3.8.1",
    "react": "next",
    "react-dom": "next",
    "react-relay": "20.1.1",
    "relay-compiler": "20.1.1",
    "relay-test-utils": "20.1.1",
    "rxjs": "7.8.2",
    "storybook": "10.2.0",
    "storybook-addon-apollo-client": "10.0.0",
    "type-fest": "5.4.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vercel": "50.9.6",
    "vite": "7.3.1",
    "vitest": "^4.0.15"
  },
  "packageManager": "yarn@4.12.0"
}
