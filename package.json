{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "suspense",
    "react suspense",
    "dashboard",
    "github"
  ],
  "engines": {},
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "echo you may want to run `yarn dev` for local development... &&  next start",
    "build:relay": "relay-compiler && prettier -w relay/__generated__ queries/__generated__",
    "build:resolvers": "node schema/wrapGraphQLSchema && graphql-codegen --config schema/codegen.yml",
    "codegen": "yarn run build:relay && yarn run build:resolvers",
    "format": "prettier . --cache -l -w",
    "vercel-build": "yarn run codegen && yarn run build",
    "vercel-build:without-chromatic": "yarn run vercel-build",
    "lint": "eslint .",
    "test": "jest --cache",
    "coverage": "yarn run test --coverage",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "storybook dev -p 9009",
    "build:storybook": "storybook build -o public/story-book --quiet"
  },
  "relay": {
    "src": ".",
    "language": "typescript",
    "exclude": [
      "/.next/**",
      "**/stuff/**",
      "**/node_modules/**",
      "**/test/",
      "**/__mocks__/",
      "**/__generated__/**"
    ],
    "schema": "./schema/schema.graphql"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@chakra-ui/next-js": "2.2.0",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@next/mdx": "14.1.4",
    "@octokit/graphql-schema": "15.20.0",
    "@octokit/rest": "20.1.1",
    "framer-motion": "11.2.10",
    "graphql": "16.8.1",
    "graphql-yoga": "5.3.0",
    "hitchcock": "0.5.0",
    "meros": "1.3.0",
    "next": "14.1.4",
    "react-cache": "latest",
    "relay-runtime": "16.2.0"
  },
  "devDependencies": {
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/storybook-addon": "5.1.0",
    "@chakra-ui/system": "2.6.2",
    "@chromatic-com/storybook": "1.5.0",
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-brands-svg-icons": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-document-nodes": "4.0.6",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-tools/schema": "10.0.3",
    "@octokit/types": "13.5.0",
    "@storybook/addon-essentials": "8.1.10",
    "@storybook/addon-storyshots": "7.6.17",
    "@storybook/addons": "7.6.17",
    "@storybook/nextjs": "8.1.10",
    "@storybook/react": "^8.0.0",
    "@types/react-relay": "16.0.6",
    "@types/relay-runtime": "14.1.23",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "babel-plugin-relay": "16.2.0",
    "chromatic": "11.5.4",
    "csstype": "3.1.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "0.25.1",
    "eslint-plugin-storybook": "0.8.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "nextjs-toploader": "1.6.11",
    "prettier": "3.2.5",
    "react": "next",
    "react-dom": "next",
    "react-relay": "16.2.0",
    "relay-compiler": "16.2.0",
    "relay-compiler-language-typescript": "15.0.1",
    "storybook": "8.1.10",
    "ts-jest": "29.1.2",
    "typescript": "5.4.5"
  },
  "packageManager": "yarn@4.3.1"
}
