{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "type": "module",
  "keywords": [
    "graphql",
    "relay",
    "react",
    "suspense",
    "react suspense",
    "dashboard",
    "github"
  ],
  "engines": {},
  "scripts": {
    "dev": "next dev",
    "circle:setup": "vercel link -y --repo --debug --project gh-dashboard-relay -t $VERCEL_TOKEN ; vercel -t $VERCEL_TOKEN pull",
    "circle:deploy": "vercel -y -t $VERCEL_TOKEN deploy --prebuilt",
    "build": "yarn run codegen && yarn next build",
    "start": "echo you may want to run `yarn dev` for local development... &&  next start",
    "build:relay": "relay-compiler && prettier -w relay/__generated__ queries/__generated__",
    "build:resolvers": "node schema/wrapGraphQLSchema && graphql-codegen --config schema/codegen.yml",
    "codegen": "yarn run build:relay && yarn run build:resolvers",
    "format": "prettier . --cache -l -w",
    "vercel-build:without-chromatic": "yarn run build",
    "lint": "eslint .",
    "test": "jest --cache",
    "coverage": "yarn run test --coverage",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "storybook dev -p 9009",
    "build:storybook": "storybook build -o public/story-book --quiet"
  },
  "relay": {
    "src": ".",
    "language": "typescript",
    "exclude": [
      "/.next/**",
      "**/stuff/**",
      "**/node_modules/**",
      "**/test/",
      "**/__mocks__/",
      "**/__generated__/**"
    ],
    "schema": "./schema/schema.graphql"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@chakra-ui/next-js": "2.4.2",
    "@chakra-ui/react": "2.10.3",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@next/mdx": "14.2.15",
    "@octokit/graphql-schema": "15.25.0",
    "@octokit/rest": "21.0.2",
    "framer-motion": "11.11.9",
    "graphql": "16.9.0",
    "graphql-yoga": "5.7.0",
    "hitchcock": "0.5.0",
    "meros": "1.3.0",
    "next": "14.2.15",
    "react-cache": "latest",
    "relay-runtime": "18.1.0"
  },
  "devDependencies": {
    "@chakra-ui/icons": "2.2.4",
    "@chakra-ui/storybook-addon": "5.2.5",
    "@chakra-ui/system": "2.6.2",
    "@chromatic-com/storybook": "2.0.2",
    "@eslint/compat": "1.2.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.13.0",
    "@fortawesome/fontawesome-svg-core": "6.6.0",
    "@fortawesome/free-brands-svg-icons": "6.6.0",
    "@fortawesome/free-solid-svg-icons": "6.6.0",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/typescript": "4.1.1",
    "@graphql-codegen/typescript-document-nodes": "4.0.11",
    "@graphql-codegen/typescript-operations": "4.3.1",
    "@graphql-codegen/typescript-resolvers": "4.4.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-tools/schema": "10.0.7",
    "@octokit/types": "13.6.1",
    "@storybook/addon-essentials": "8.3.6",
    "@storybook/addon-storyshots": "7.6.17",
    "@storybook/addons": "7.6.17",
    "@storybook/nextjs": "8.3.6",
    "@storybook/react": "^8.0.0",
    "@types/react-relay": "16.0.6",
    "@types/relay-runtime": "18.1.0",
    "@typescript-eslint/eslint-plugin": "8.10.0",
    "@typescript-eslint/parser": "8.10.0",
    "babel-plugin-relay": "18.1.0",
    "chromatic": "11.12.6",
    "csstype": "3.1.3",
    "eslint": "9.13.0",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-sonarjs": "2.0.4",
    "eslint-plugin-storybook": "0.9.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "nextjs-toploader": "3.7.15",
    "prettier": "3.3.3",
    "react": "next",
    "react-dom": "next",
    "react-relay": "18.0.0",
    "relay-compiler": "18.1.0",
    "relay-compiler-language-typescript": "15.0.1",
    "storybook": "8.3.6",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3",
    "typescript-eslint": "8.10.0",
    "vercel": "37.11.0"
  },
  "packageManager": "yarn@4.5.1"
}
