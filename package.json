{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "dashboard",
    "docker",
    "github"
  ],
  "scripts": {
    "test": "NODE_ENV=test istanbul cover -i '**/*.js' -i '**/*.jsx' _mocha test -- --compilers jsx:babel-core/register",
    "test:circle": "XUNIT_FILE=$CIRCLE_TEST_REPORTS/mocha-xunit.xml npm run test -- -R xunit-file",
    "prestart": "webpack",
    "start": "node ."
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": "https://github.com/lowsky/dashboard",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.0.1",
    "xunit-file": "0.0.9"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "jsdom": "^8.2.0",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "superagent": "^1.8.3",
    "webpack": "^1.12.13"
  }
}
