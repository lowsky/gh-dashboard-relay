{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "suspense",
    "react suspense",
    "dashboard",
    "github"
  ],
  "engines": {},
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "echo you may want to run `yarn dev` for local development... &&  next start",
    "build:relay": "relay-compiler && prettier -w relay/__generated__ queries/__generated__",
    "build:resolvers": "node schema/wrapGraphQLSchema && graphql-codegen --config schema/codegen.yml",
    "codegen": "yarn run build:relay && yarn run build:resolvers",
    "vercel-build": "yarn run codegen && yarn run build",
    "vercel-build:without-chromatic": "yarn run vercel-build",
    "lint": "eslint .",
    "test": "jest --cache",
    "coverage": "yarn run test --coverage",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "storybook dev -p 9009",
    "build:storybook": "storybook build -o public/story-book --quiet"
  },
  "relay": {
    "src": ".",
    "language": "typescript",
    "exclude": [
      "/.next/**",
      "**/stuff/**",
      "**/node_modules/**",
      "**/test/",
      "**/__mocks__/",
      "**/__generated__/**"
    ],
    "schema": "./schema/schema.graphql"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.4",
    "@chakra-ui/react": "2.8.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@next/mdx": "13.4.19",
    "@octokit/graphql-schema": "14.31.0",
    "@octokit/rest": "20.0.1",
    "framer-motion": "10.16.4",
    "graphql": "16.8.0",
    "graphql-yoga": "4.0.4",
    "hitchcock": "0.5.0",
    "meros": "1.3.0",
    "next": "13.4.19",
    "react-cache": "latest",
    "relay-runtime": "15.0.0"
  },
  "devDependencies": {
    "@chakra-ui/icons": "2.1.0",
    "@chakra-ui/storybook-addon": "5.0.1",
    "@chakra-ui/system": "2.6.0",
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-document-nodes": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@graphql-tools/schema": "10.0.0",
    "@octokit/types": "11.1.0",
    "@storybook/addon-essentials": "7.4.2",
    "@storybook/addon-storyshots": "7.4.2",
    "@storybook/addons": "7.4.2",
    "@storybook/nextjs": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@types/react-relay": "14.1.5",
    "@types/relay-runtime": "14.1.13",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "babel-plugin-relay": "15.0.0",
    "chromatic": "6.24.1",
    "csstype": "3.1.2",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-storybook": "0.6.13",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "nextjs-toploader": "1.4.2",
    "prettier": "3.0.3",
    "react": "next",
    "react-dom": "next",
    "react-relay": "15.0.0",
    "relay-compiler": "15.0.0",
    "relay-compiler-language-typescript": "15.0.1",
    "storybook": "7.4.2",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@3.6.3"
}
