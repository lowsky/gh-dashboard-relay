{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "github"
  ],
  "engines": {},
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "echo you may want to run `yarn dev` for local development... &&  next start",
    "build:relay": "relay-compiler && prettier -w relay/__generated__ queries/__generated__",
    "build:resolvers": "node schema/wrapGraphQLSchema && graphql-codegen --config schema/codegen.yml",
    "codegen": "yarn run build:relay && yarn run build:resolvers",
    "vercel-build": "yarn run codegen && yarn run build",
    "vercel-build:without-chromatic": "yarn run vercel-build",
    "lint": "eslint .",
    "test": "jest --cache",
    "coverage": "yarn run test --coverage",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "storybook dev -p 9009",
    "build:storybook": "storybook build -o public/story-book --quiet"
  },
  "relay": {
    "src": ".",
    "language": "typescript",
    "exclude": [
      "/.next/**",
      "**/stuff/**",
      "**/node_modules/**",
      "**/test/",
      "**/__mocks__/",
      "**/__generated__/**"
    ],
    "schema": "./schema/schema.graphql"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "@chakra-ui/react": "2.5.5",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next/mdx": "13.2.4",
    "@octokit/graphql-schema": "13.7.0",
    "@octokit/rest": "19.0.7",
    "framer-motion": "10.9.1",
    "graphql": "16.0.0-rc.7.experimental-stream-defer.4",
    "graphql-yoga": "3.8.0",
    "meros": "1.2.1",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "relay-runtime": "15.0.0",
    "simple-cache-provider": "0.10.0"
  },
  "devDependencies": {
    "@chakra-ui/icons": "2.0.18",
    "@chakra-ui/storybook-addon": "4.0.17",
    "@chakra-ui/system": "2.5.5",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-document-nodes": "3.0.2",
    "@graphql-codegen/typescript-operations": "3.0.2",
    "@graphql-codegen/typescript-resolvers": "3.1.1",
    "@graphql-tools/schema": "9.0.17",
    "@octokit/types": "9.0.0",
    "@storybook/addon-essentials": "7.0.0-rc.1",
    "@storybook/addon-storyshots": "7.0.0-rc.1",
    "@storybook/addons": "7.0.0-rc.1",
    "@storybook/nextjs": "^7.0.0-beta.34",
    "@storybook/react": "^7.0.0-beta.34",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@types/react-relay": "14.1.3",
    "@types/relay-runtime": "14.1.9",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "babel-plugin-relay": "15.0.0",
    "chromatic": "6.17.2",
    "csstype": "3.1.1",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-storybook": "0.6.11",
    "hitchcock": "0.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "react-relay": "15.0.0",
    "relay-compiler": "15.0.0",
    "relay-compiler-language-typescript": "15.0.1",
    "storybook": "7.0.0-rc.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.5.0"
}
