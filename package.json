{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=8"
  },
  "proxy": "http://localhost:3000",
  "scripts": {
    "cra-start": "PORT=8080 react-app-rewired start",
    "cra-build": "react-app-rewired build",
    "relay": "relay-compiler --src ./src --schema ./src/relay/data/schema.graphql",
    "build": "npm run relay ; npm run cra-build ; npm run build-storybook -- -o build/story-book",
    "lint": "eslint .",
    "dev": "npm run cra-start",
    "test": "react-app-rewired test --env=jsdom --no-cache ",
    "coverage": "npm run test -- --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml npm test -- --ci --testResultsProcessor=./node_modules/jest-junit-reporter --forceExit",
    "start": "babel-node .",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "babel-runtime": "6.26.0",
    "bulma": "^0.7.1",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "helmet": "3.12.1",
    "react-scripts": "1.1.2",
    "relay-runtime": "1.5.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.10",
    "@storybook/addon-links": "3.4.10",
    "@storybook/addons": "3.4.10",
    "@storybook/react": "3.4.10",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-plugin-relay": "1.6.0",
    "babel-register": "6.26.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-graphql": "2.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "graphql": "0.13.2",
    "graphql-relay": "0.5.5",
    "jest-junit-reporter": "1.1.0",
    "prettier": "1.13.7",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-app-rewired": "1.5.2",
    "react-dom": "16.4.1",
    "react-relay": "1.6.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.4.1",
    "relay-compiler": "1.6.0",
    "svg-url-loader": "2.3.2",
    "xunit-file": "1.0.0"
  }
}
