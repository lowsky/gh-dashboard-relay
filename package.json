{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "type": "module",
  "keywords": [
    "graphql",
    "relay",
    "react",
    "suspense",
    "react suspense",
    "dashboard",
    "github"
  ],
  "engines": {},
  "scripts": {
    "dev": "next dev",
    "circle:setup": "vercel link -y --repo --debug --project gh-dashboard-relay -t $VERCEL_TOKEN ; vercel -t $VERCEL_TOKEN pull",
    "circle:deploy": "vercel -y -t $VERCEL_TOKEN deploy --prebuilt",
    "build": "yarn run codegen && yarn next build",
    "start": "echo you may want to run `yarn dev` for local development... &&  next start",
    "build:relay": "relay-compiler && prettier -w relay/__generated__ components/__generated__ container/__generated__ relay/__generated__ app/relay/[userName]/__generated__ app/relay/[userName]/[repoName] ",
    "codegen": "yarn run build:relay",
    "format": "prettier . --cache -l -w",
    "vercel-build:without-chromatic": "yarn run build",
    "lint": "eslint .",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "storybook dev -p 9009",
    "build:storybook": "storybook build -o public/story-book --quiet"
  },
  "relay": {
    "src": ".",
    "language": "typescript",
    "exclude": [
      "/.next/**",
      "**/stuff/**",
      "**/node_modules/**",
      "**/test/",
      "**/__generated__/**"
    ],
    "schema": "./schema.graphql"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@chakra-ui/react": "3.19.0",
    "@emotion/react": "11.14.0",
    "@next/mdx": "15.3.2",
    "@octokit/graphql-schema": "15.26.0",
    "graphql": "16.11.0",
    "meros": "1.3.0",
    "next": "15.3.2",
    "next-themes": "^0.4.3",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.6",
    "@eslint/compat": "1.2.9",
    "@eslint/js": "9.27.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@graphql-eslint/eslint-plugin": "^4.0.0",
    "@graphql-tools/schema": "10.0.23",
    "@next/eslint-plugin-next": "^15.3.2",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-themes": "8.6.14",
    "@storybook/nextjs": "8.6.14",
    "@storybook/react": "8.6.14",
    "@types/react": "19.1.4",
    "@types/react-relay": "^18.2.0",
    "@types/relay-test-utils": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "chromatic": "11.28.2",
    "eslint": "9.27.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-storybook": "0.12.0",
    "globals": "16.1.0",
    "nextjs-toploader": "3.8.16",
    "prettier": "3.5.3",
    "react": "next",
    "react-dom": "next",
    "react-relay": "19.0.0",
    "relay-compiler": "19.0.0",
    "relay-test-utils": "19.0.0",
    "storybook": "8.6.14",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1",
    "vercel": "41.7.8"
  },
  "packageManager": "yarn@4.9.1"
}
