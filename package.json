{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "A web app showing the states of all (feature) branches and links to running docker containers for each feature.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "keywords": [
    "graphql",
    "relay",
    "react",
    "dashboard",
    "docker",
    "github"
  ],
  "engines": {
    "node": ">=8"
  },
  "proxy": "http://localhost:3000",
  "scripts": {
    "cra-start": "PORT=8080 react-scripts start",
    "cra-build": "react-scripts build",
    "relay": "relay-compiler --src ./src --schema ./src/relay/data/schema.graphql",
    "build": "npm run relay ; npm run cra-build ; npm run build-storybook -- -o build/story-book",
    "lint": "eslint .",
    "dev": "npm run cra-start",
    "test": "react-scripts test --no-cache ",
    "coverage": "npm run test -- --coverage --coveragePathIgnorePatterns=src/relay/__generated__",
    "test:circleci": "TEST_REPORT_FILENAME=test-reports/jest-junit.xml npm test -- --ci --testResultsProcessor=./node_modules/jest-junit-reporter --forceExit",
    "start": "babel-node .",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "analyze": true,
  "homepage": ".",
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "bulma": "0.7.2",
    "codacy-coverage": "3.1.0",
    "cors": "2.8.5",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-graphql": "0.6.12",
    "github-api": "lowsky/github-api#supportInUsedInBrowser",
    "helmet": "3.14.0",
    "react-scripts": "2.0.5",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.4",
    "@storybook/addon-links": "4.0.4",
    "@storybook/addons": "4.0.4",
    "@storybook/react": "4.0.4",
    "babel-loader": "8.0.4",
    "babel-plugin-relay": "1.7.0-rc.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-graphql": "2.1.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "graphql": "0.13.2",
    "graphql-relay": "0.5.5",
    "jest-junit-reporter": "1.1.0",
    "prettier": "1.14.3",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-relay": "1.6.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.5.2",
    "relay-compiler": "1.6.2",
    "xunit-file": "1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
