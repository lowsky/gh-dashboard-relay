{
  "name": "dashboard",
  "version": "2.0.0",
  "description": "A web site showing the build states of all (feature) branches on one page.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lowsky/dashboard.git"
  },
  "type": "module",
  "keywords": [
    "graphql",
    "relay",
    "react",
    "suspense",
    "react suspense",
    "dashboard",
    "github"
  ],
  "engines": {},
  "scripts": {
    "dev": "next dev",
    "circle:setup": "vercel link -y --repo --debug --project gh-dashboard-relay -t $VERCEL_TOKEN ; vercel -t $VERCEL_TOKEN pull",
    "circle:deploy": "vercel -y -t $VERCEL_TOKEN deploy --prebuilt",
    "build": "yarn run codegen && yarn next build",
    "start": "echo you may want to run `yarn dev` for local development... &&  next start",
    "build:relay": "relay-compiler && prettier -w relay/__generated__ queries/__generated__",
    "build:resolvers": "node schema/wrapGraphQLSchema && graphql-codegen --config schema/codegen.yml",
    "codegen": "yarn run build:relay && yarn run build:resolvers",
    "format": "prettier . --cache -l -w",
    "vercel-build:without-chromatic": "yarn run build",
    "lint": "eslint .",
    "test": "jest --cache",
    "coverage": "yarn run test --coverage",
    "chromatic-test": "chromatic --exit-zero-on-changes -d public/story-book --ci --allow-console-errors --exit-once-uploaded '*'",
    "storybook": "storybook dev -p 9009",
    "build:storybook": "storybook build -o public/story-book --quiet"
  },
  "relay": {
    "src": ".",
    "language": "typescript",
    "exclude": [
      "/.next/**",
      "**/stuff/**",
      "**/node_modules/**",
      "**/test/",
      "**/__mocks__/",
      "**/__generated__/**"
    ],
    "schema": "./schema/schema.graphql"
  },
  "author": "Robert Hostlowsky",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/lowsky/dashboard/issues"
  },
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@chakra-ui/react": "3.15.1",
    "@emotion/react": "11.14.0",
    "@next/mdx": "15.3.0",
    "@octokit/graphql-schema": "15.26.0",
    "@octokit/rest": "21.1.1",
    "graphql": "16.10.0",
    "graphql-yoga": "5.13.3",
    "meros": "1.3.0",
    "next": "15.3.0",
    "next-themes": "^0.4.3",
    "react-cache": "latest",
    "react-icons": "^5.3.0",
    "relay-runtime": "18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.6",
    "@eslint/compat": "1.2.8",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.24.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-document-nodes": "4.0.16",
    "@graphql-codegen/typescript-operations": "4.6.0",
    "@graphql-codegen/typescript-resolvers": "4.5.0",
    "@graphql-eslint/eslint-plugin": "^4.0.0",
    "@graphql-tools/schema": "10.0.23",
    "@octokit/types": "13.10.0",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-themes": "8.6.12",
    "@storybook/nextjs": "8.6.12",
    "@storybook/react": "8.6.12",
    "@types/react": "19.1.1",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "babel-plugin-relay": "18.2.0",
    "chromatic": "11.28.0",
    "csstype": "3.1.3",
    "eslint": "9.24.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-storybook": "0.12.0",
    "globals": "16.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "nextjs-toploader": "3.8.16",
    "prettier": "3.5.3",
    "react": "next",
    "react-dom": "next",
    "react-relay": "18.2.0",
    "relay-compiler": "18.2.0",
    "storybook": "8.6.12",
    "ts-jest": "29.3.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vercel": "41.6.0"
  },
  "packageManager": "yarn@4.9.0"
}
